package poli;

public class Dog extends Animal {
	
	public Dog(String name) {
		super(name);
		// TODO Auto-generated constructor stub
	}

	@Override
	 void giveVoice() {
		System.out.println("Jestem psem i nazywam sie:  " + getName());
	}
	
	public void bark() {
		System.out.println("Hau Hau");
	}

}
