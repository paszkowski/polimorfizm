package poli;

public class Animal {
	
private String name;
	
	void giveVoice() {
		
		System.out.println("Jestem zwierzeciem i nazywam sie:  " + getName());
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
	
	public Animal(String name) {
		setName(name);
	}

}
